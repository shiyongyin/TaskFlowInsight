    /**
     * æ‰‹åŠ¨æ¯”è¾ƒMapï¼ˆç®€åŒ–ç‰ˆï¼Œä»…æ¯”è¾ƒValueï¼‰
     */
    private static <K, V> CompareResult compareMapManually(Map<K, V> oldMap, Map<K, V> newMap) {
        List<FieldChange> changes = new ArrayList<>();

        Set<K> allKeys = new LinkedHashSet<>();
        allKeys.addAll(oldMap.keySet());
        allKeys.addAll(newMap.keySet());

        for (K key : allKeys) {
            V oldValue = oldMap.get(key);
            V newValue = newMap.get(key);

            if (oldValue == null && newValue != null) {
                // CREATE
                changes.add(createChange("map[" + formatKey(key) + "]", ChangeType.CREATE, null, newValue));
            } else if (oldValue != null && newValue == null) {
                // DELETE
                changes.add(createChange("map[" + formatKey(key) + "]", ChangeType.DELETE, oldValue, null));
            } else if (oldValue != null && newValue != null) {
                // UPDATE: æ·±åº¦æ¯”è¾ƒValue
                compareValues(key, oldValue, newValue, changes);
            }
        }

        return CompareResult.builder()
            .changes(changes)
            .identical(changes.isEmpty())
            .similarity(changes.isEmpty() ? 1.0 : 0.5)
            .build();
    }

    /**
     * æ·±åº¦æ¯”è¾ƒValueï¼ˆæ ¸å¿ƒé€»è¾‘ï¼‰
     */
    private static <K, V> void compareValues(K key, V oldValue, V newValue, List<FieldChange> changes) {
        String keyPath = "map[" + formatKey(key) + "]";

        if (oldValue instanceof Product && newValue instanceof Product) {
            compareProduct(keyPath, (Product) oldValue, (Product) newValue, changes);
        } else if (oldValue instanceof Warehouse && newValue instanceof Warehouse) {
            compareWarehouse(keyPath, (Warehouse) oldValue, (Warehouse) newValue, changes);
        } else if (oldValue instanceof UserWithInclude && newValue instanceof UserWithInclude) {
            compareUserWithInclude(keyPath, (UserWithInclude) oldValue, (UserWithInclude) newValue, changes);
        } else if (oldValue instanceof OrderWithIgnore && newValue instanceof OrderWithIgnore) {
            compareOrderWithIgnore(keyPath, (OrderWithIgnore) oldValue, (OrderWithIgnore) newValue, changes);
        } else if (!Objects.equals(oldValue, newValue)) {
            changes.add(createChange(keyPath, ChangeType.UPDATE, oldValue, newValue));
        }
    }

    /**
     * æ¯”è¾ƒProductï¼ˆåŒ…å«åµŒå¥—Entityå’ŒValueObjectï¼‰
     */
    private static void compareProduct(String path, Product old, Product neu, List<FieldChange> changes) {
        // åŸºæœ¬å­—æ®µ
        if (!Objects.equals(old.getName(), neu.getName())) {
            changes.add(createChange(path + ".name", ChangeType.UPDATE, old.getName(), neu.getName()));
        }
        if (!Objects.equals(old.getPrice(), neu.getPrice())) {
            changes.add(createChange(path + ".price", ChangeType.UPDATE, old.getPrice(), neu.getPrice()));
        }
        if (!Objects.equals(old.getStock(), neu.getStock())) {
            changes.add(createChange(path + ".stock", ChangeType.UPDATE, old.getStock(), neu.getStock()));
        }

        // åµŒå¥—Entityï¼ˆæ·±åº¦æ¯”è¾ƒï¼‰
        compareSupplier(path + ".supplier", old.getSupplier(), neu.getSupplier(), changes);

        // åµŒå¥—Entityï¼ˆShallowReferenceï¼‰
        compareWarehouseShallow(path + ".warehouse", old.getWarehouse(), neu.getWarehouse(), changes);

        // åµŒå¥—ValueObject
        compareAddress(path + ".shippingAddress", old.getShippingAddress(), neu.getShippingAddress(), changes);
    }

    /**
     * æ¯”è¾ƒSupplierï¼ˆæ·±åº¦æ¯”è¾ƒï¼‰
     */
    private static void compareSupplier(String path, Supplier old, Supplier neu, List<FieldChange> changes) {
        if (old == null && neu == null) return;
        if (old == null || neu == null) {
            changes.add(createChange(path, ChangeType.UPDATE, old, neu));
            return;
        }

        // åŒIDæ·±åº¦æ¯”è¾ƒ
        if (Objects.equals(old.getSupplierId(), neu.getSupplierId())) {
            if (!Objects.equals(old.getName(), neu.getName())) {
                changes.add(createChange(path + ".name", ChangeType.UPDATE, old.getName(), neu.getName()));
            }
            if (!Objects.equals(old.getCity(), neu.getCity())) {
                changes.add(createChange(path + ".city", ChangeType.UPDATE, old.getCity(), neu.getCity()));
            }
            if (!Objects.equals(old.getState(), neu.getState())) {
                changes.add(createChange(path + ".state", ChangeType.UPDATE, old.getState(), neu.getState()));
            }
        } else {
            // ä¸åŒIDï¼Œå¼•ç”¨æ›¿æ¢
            changes.add(createChange(path, ChangeType.UPDATE, old, neu));
        }
    }

    /**
     * æ¯”è¾ƒWarehouseï¼ˆShallowReferenceï¼Œä»…æ¯”è¾ƒKeyï¼‰
     */
    private static void compareWarehouseShallow(String path, Warehouse old, Warehouse neu, List<FieldChange> changes) {
        if (old == null && neu == null) return;
        if (old == null || neu == null) {
            changes.add(createChange(path, ChangeType.UPDATE, old, neu));
            return;
        }

        // ShallowReferenceï¼šä»…æ¯”è¾ƒKey
        boolean keyChanged = !Objects.equals(old.getWarehouseId(), neu.getWarehouseId()) ||
                             !Objects.equals(old.getRegionCode(), neu.getRegionCode());

        if (keyChanged) {
            changes.add(createChange(path + ".key", ChangeType.UPDATE,
                formatWarehouseKey(old), formatWarehouseKey(neu)));
        }
        // å…¶ä»–å­—æ®µï¼ˆlocation, capacityï¼‰ä¸æ¯”è¾ƒ
    }

